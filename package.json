{
	"name": "customizablediscordbot",
	"version": "1.0.0",
	"description": "A customizable Discord general-purpose bot, written in TypeScript",
	"main": "dist/index.js",
	"scripts": {
		"start": "node dist/index.js",
		"startdev": "DEBUG=1 node dist/index.js",
		"build": "tsc --build",
		"deploy": "npm run build && npm run start",
		"deploydev": "npm run build && npm run startdev",
		"dev": "nodemon --exec \"npm run deploy\" -e \"ts\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GaryCraft/customizableDiscordBot.git"
	},
	"author": "GaryCraft",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/GaryCraft/customizableDiscordBot/issues"
	},
	"homepage": "https://github.com/GaryCraft/customizableDiscordBot#readme",
	"devDependencies": {
		"@types/node": "^17.0.25",
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"eslint": "^8.13.0",
		"sqlite3": "^5.0.8",
		"typescript": "^4.8.0-dev.20220613"
	},
	"dependencies": {
		"discord.js": "^13.8.0",
		"mysql2": "^2.3.3",
		"typeorm": "^0.3.6",
		"uuid": "^8.3.2",
		"winston": "^3.7.2"
	}
}
