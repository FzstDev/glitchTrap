{
	"name": "fzstbot",
	"version": "1.3.4",
	"description": "A custom Discord specific-purpose bot, written in TypeScript, for the Fzst Discord Server.",
	"main": "dist/index.js",
	"scripts": {
		"start": "node dist/index.js",
		"startdev": "DEBUG=1 node dist/index.js",
		"build": "tsc --build",
		"deploy": "npm run build && npm run start",
		"deploydev": "npm run build && npm run startdev",
		"dev": "npx nodemon --exec \"npm run deploy\" -e \"ts\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GaryCraft/customizableDiscordBot.git"
	},
	"author": "GaryCraft",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/GaryCraft/customizableDiscordBot/issues"
	},
	"homepage": "https://github.com/GaryCraft/customizableDiscordBot#readme",
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/node": "^17.0.25",
		"@types/node-schedule": "^2.1.0",
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"eslint": "^8.13.0",
		"sqlite3": "^5.0.8",
		"typescript": "^4.8.0-dev.20220613"
	},
	"dependencies": {
		"@haileybot/captcha-generator": "^1.7.0",
		"@spaceproject/findrecursive": "^1.0.3",
		"discord.js": "^13.11.0",
		"express": "^4.18.1",
		"mysql2": "^2.3.3",
		"node-schedule": "^2.1.0",
		"typeorm": "^0.3.10",
		"uuid": "^8.3.2",
		"winston": "^3.7.2"
	}
}
